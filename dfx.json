{
    "canisters": {
        "blueband_rust": {
            "type": "rust",
            "package": "blueband_rust",
            "candid": "blueband_rust.did",
            "build": [
                "cargo build --target wasm32-unknown-unknown --release --package blueband_rust"
            ],
            "wasm": "target/wasm32-unknown-unknown/release/blueband_rust.wasm",
            "gzip": true,
            "declarations": {
                "output": "declarations"
            },
            "metadata": [
                {
                    "name": "candid:service",
                    "path": "blueband_rust.did"
                }
            ]
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:4943",
            "type": "ephemeral",
            "replica": {
                "subnet_type": "system"
            }
        },
        "ic": {
            "providers": [
                "https://icp0.io"
            ],
            "type": "persistent"
        }
    },
    "output_env_file": ".env",
    "version": 1
}